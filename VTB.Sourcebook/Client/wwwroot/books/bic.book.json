{
    "name": "bic",
    "title": "Справочник БИК",
    "filters": [
        { "field": "bic_code", "editor": "Text", "title": "БИК" },
        { "field": "participant_name", "editor": "Text", "title": "Участник" },
        { "field": "participant_type", "editor": "List", "list": "participant", "title": "Тип участника" },
        { "field": "locality_name", "editor": "List", "list": "locality", "title": "Город" },
        { "field": "date_in", "editor": "DateRange", "title": "Дата создания" }
    ],
    "columns": [
        { "title": "БИК", "value": "<a href='Redirect/bic/?bic_code={bic_code}'>{bic_code}</a>", "sortBy": "bic_code", "filter": "bic_code" },
        { "title": "Участник", "value": "{participant_name}", "sortBy": "participant_name", "hints_": "Hidden", "filter": "participant_name" },
        { "title": "Тип участника", "value": "{participant_type}", "sortBy": "participant_type", "filter": "participant_type" },
        { "title": "Город", "value": "{locality_type} {locality_name}", "sortBy": "locality_name", "filter": "locality_name" },
        { "title": "Дата создания", "value": "{date_in:dd.MM.yyyy}", "sortBy": "date_in", "filter": "date_in" },
        { "title": "Дата удаления", "value": "{date_out:dd.MM.yyyy}", "sortBy": "date_out" }
    ],
    "fields": [
        { "name": "dictionary.bic.bic_code" },
        { "name": "a.participant_name" },
        { "name": "a.participant_type" },
        { "name": "a.locality_name" },
        { "name": "a.locality_type" },
        { "name": "a.date_in" },
        { "name": "a.date_out" }
    ],
    "tables": [
        {
            "name": "dictionary.bic",
            "key": "bic",
            "alias": "a"
        }
    ],
    "condition_": "business_day != null && participant_name like 'A%'",
    "lists": [
        {
            "name": "participant",
            "valueType": "Integer",
            "values": [
                { "text": "0",  "value": "Главное управление Банка России" },
                { "text": "10", "value": "Расчетно-кассовый центр" },
                { "text": "12", "value": "Отделение, отделение – национальный банк главного управления Банка России" },
                { "text": "15", "value": "Структурное подразделение центрального аппарата Банка России" },
                { "text": "16", "value": "Кассовый центр" },
                { "text": "20", "value": "Кредитная организация" },
                { "text": "30", "value": "Филиал кредитной организации" },
                { "text": "40", "value": "Полевое учреждение Банка России" },
                { "text": "51", "value": "Федеральное казначейство" },
                { "text": "52", "value": "Территориальный орган Федерального казначейства" },
                { "text": "60", "value": "Иностранный банк (иностранная кредитная организация)" },
                { "text": "65", "value": "Иностранный центральный (национальный) банк" },
                { "text": "71", "value": "Клиент кредитной организации, являющийся косвенным участником" },
                { "text": "75", "value": "Клиринговая организация" },
                { "text": "78", "value": "Внешняя платежная система" },
                { "text": "90", "value": "Конкурсный управляющий (ликвидатор, ликвидационная комиссия)" },
                { "text": "99", "value": "Клиент Банка России, не являющийся участником платежной системы" }
            ]
        },
        {
            "name": "locality",
            "valueType": "String",
            "source": {
                "fields": [
                    { "name": "a.locality_name", "alias": "text" },
                    { "name": "a.locality_name", "alias": "value" }
                ],
                "tables": [
                  {
                    "name": "dictionary.bic",
                    "alias": "a"
                  }
                ],
                "distinct": true,
                "sortBy": "text",
                "condition": "value != null"
            }
        }
    ]
}
