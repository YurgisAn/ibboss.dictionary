        {
            "name": "locality",
            "valueType": "String",
            "source": {
                "fields": [
                    { "name": "a.locality_name", "alias": "text" },
                    { "name": "a.locality_name", "alias": "value" }
                ],
                "tables": [
                  {
                    "name": "dictionary.bic",
                    "alias": "a"
