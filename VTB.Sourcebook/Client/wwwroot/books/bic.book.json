                { "value": "16", "text": "Кассовый центр" },
                { "value": "20", "text": "Кредитная организация" },
                { "value": "30", "text": "Филиал кредитной организации" },
                { "value": "40", "text": "Полевое учреждение Банка России" },
                { "value": "51", "text": "Федеральное казначейство" },
